CREATE TABLE OWNER(
	ownerID INT PRIMARY KEY,
	name VARCHAR(30) NOT NULL
);

CREATE TABLE BOAT(
	name VARCHAR(30) PRIMARY KEY,
	owner INT REFERENCES OWNER(ownerID),
	size INT CHECK (size > 0),
	color VARCHAR(10) NOT NULL
);

CREATE TABLE CONTACT(
	owner_a INT REFERENCES OWNER(ownerID),
	owner_b INT REFERENCES OWNER(ownerID)
);
